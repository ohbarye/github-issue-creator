{
  "manifest_version": 2,
  "name": "Issue Creator",
  "description": "This extension helps you to create GitHub issues.",
  "version": "0.1.0",
  "icons": {
    //"16": "",
    //"48": "",
    //"128": ""
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/icon_19.png"
      //"38": "assets/icon_19.png"
    },
    "default_title": "Create new Issue",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "permissions": [
    "tabs",
    "storage"
  ]
}
